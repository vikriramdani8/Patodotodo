<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patodotodo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activity_tambahcategory"></activity>
        <activity android:name=".activity_profile" />
        <activity android:name=".activity_mylist" />
        <activity android:name=".activity_registration" />
        <activity android:name=".activity_detail" />
        <activity
            android:name=".activity_tambah"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".activity_menu" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>